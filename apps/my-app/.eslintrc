{
  "extends": [
    "react-app",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier":["error", {
      "useTabs": false,
      "printWidth": 120,
      "tabWidth": 2,
      "singleQuote": true,
      "bracketSpacing": true,
      "arrowParens": "avoid",
      "trailingComma" : "es5",
      "jsxBracketSameLine": true,
      "parser":"typescript"
    }],
    "arrow-body-style": ["error", "as-needed"]
  }
}